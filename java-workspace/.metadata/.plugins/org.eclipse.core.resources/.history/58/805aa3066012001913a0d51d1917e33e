package com.bit.day01.homework01;

import java.util.Scanner;

public class Prob04 {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.print("문자열을 입력하세요 : ");
		String str = sc.next();
		
		for(int i=str.length(); i<0; i--) {
			System.out.println();
		}
		
		sc.close();
	}
	
}
