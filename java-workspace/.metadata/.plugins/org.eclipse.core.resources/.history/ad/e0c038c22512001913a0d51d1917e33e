package com.bit.day01;

public class VariableMain {
	
	public static void main(String[] args) {
		int num = 10;
		System.out.println(num);
		
		int a;
		a = 20;
		System.out.println(a);
		
		byte b = 10;
		short s = 100;
		int i = 10;
		long l = 1000;
		float f = 3.14;
		double d = 3.14;
		char c = 'A';
		boolean bool = true;
	}
	
}
