package com.bit.day01;

public class NamingMain {
	
	public static void main(String[] args) {
		// Camel Case
		int sumOfStudentNumber;
		
		// 헝가리안식 표기법
		// 변수의 자료형 약자를 접두어로 활용
		int iNum;
		
		// Snake Case
		int total_number;
		
		/*
		 * Kebob Case
		 * 	: -를 활용하여 단어의 의미를 구분
		 * 		*HTML에서 속성의 이름을 지을 때 주로 사용
		 */
	}
	
}
