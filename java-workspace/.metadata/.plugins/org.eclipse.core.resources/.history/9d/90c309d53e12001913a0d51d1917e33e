package com.bit.day01;

public class CastMain {
	
	public static void main(String[] args) {
	/*
		double a = 3.14;
		int b;
		b = (int) a;
		System.out.println(b);
		
		int c = 10;
		double d;
		d = c;
		System.out.println(d);
	*/
		byte b = 25;
		short s = b;
		int i = s;
		long l = i;
		float f = l;
		double d = f;

		byte b1 = (byte) 256;
		System.out.println(b1);
	}
	
}
